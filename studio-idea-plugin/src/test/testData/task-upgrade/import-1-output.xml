        </work>
        <distribution>
            <workerThreads>8</workerThreads>
            <subtasks/>
        </distribution>
        <tailoring>
            <change>
                <reference>resourceObjects</reference>
                <distribution>
                    <buckets>
                        <stringSegmentation>
                            <discriminator>attributes/ri:uid</discriminator>
                            <boundaryCharacters>0-9a-z</boundaryCharacters>
                            <depth>1</depth>
                            <comparisonMethod>prefix</comparisonMethod>
                        </stringSegmentation>
                    </buckets>
                    <workers>
                        <workersPerNode>
