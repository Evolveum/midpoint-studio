<task xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3" xmlns:apti="http://midpoint.evolveum.com/xml/ns/public/common/api-types-3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:icfs="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/resource-schema-3" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3" xmlns:mext="http://midpoint.evolveum.com/xml/ns/public/model/extension-3" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3" xmlns:t="http://prism.evolveum.com/xml/ns/public/types-3" xmlns:s="http://midpoint.evolveum.com/xml/ns/public/model/scripting-3" xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3" xmlns:org="http://midpoint.evolveum.com/xml/ns/public/common/org-3">
    <name>Import 1</name>
    <assignment>
        <targetRef oid="00000000-0000-0000-0000-000000000503" type="ArchetypeType"/>
    </assignment>
    <ownerRef oid="00000000-0000-0000-0000-000000000002" type="c:UserType"/>
    <executionState>closed</executionState>
    <executionEnvironment>
        <cachingProfile>roles-and-shadows</cachingProfile>
    </executionEnvironment>
    <activity>
        <work>
            <import>
                <resourceObjects>
                    <resourceRef oid="5a55bc1a-95c0-11ea-bc3c-abbaf8e94a00"/>
                    <kind>account</kind>
                    <intent>default</intent>
                    <objectclass>ri:somePerson</objectclass>
